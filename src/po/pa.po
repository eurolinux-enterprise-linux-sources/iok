msgid "Backspace"
msgstr "ਬੈਕਸਪੇਸ"

#: ../src/interface.c:9616
msgid "Tab"
msgstr "ਟੈਬ "

#: ../src/interface.c:9708
msgid "Caps"
msgstr "ਕੈਪਸ "

#: ../src/interface.c:9779
msgid "Enter"
msgstr "ਐਂਟਰ "

